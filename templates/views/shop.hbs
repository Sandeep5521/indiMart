  <script>
    let cat = "{{shop}}"
    let le = document.getElementById('lele')
    console.log(cat)
    const addp = (Sort) => {
      fetch(`https://dummyjson.com/products/category/${cat}`)
        .then(res => res.json())
        .then((tmp) => {
          console.log(tmp)
          let val = tmp.products
          if (Sort > 0) val.sort((a, b) => {
            return a.price - b.price
          })
          if (Sort < 0) val.sort((a, b) => {
            return b.price - a.price
          })
          console.log(val)
          le.innerHTML = ''
          for (let i = 0; i < val.length; i++) {
            le.innerHTML += `<div class="col-lg-4 col-md-6 col-sm-6" onclick="go('${cat}','${val[i].title}')">
                            <div class="product__item">
                                <div class="product__item__pic pro" style="background-image: url('${val[i].thumbnail}');">
                                </div>
                                <div class="product__item__text">
                                    <h6>${val[i].title}</h6>
                                    <a href="#" class="add-cart">Shop Now</a>
                                    <div class="rating">
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                    </div>
                                    <h5>₹${val[i].price}</h5>
                                </div>
                            </div>
                        </div>`
          }
        })
    }
    addp(0)
    const go = (c, n) => {
      open(`/product?cat=${c}&name=${n}`, '_self')
    }
  </script>
